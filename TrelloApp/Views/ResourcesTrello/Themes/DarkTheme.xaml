    <SolidColorBrush x:Key="ThemeHoverColor">#febd69</SolidColorBrush>
    <SolidColorBrush x:Key="ThemeHoverTextColor">#000000</SolidColorBrush>
    <SolidColorBrush x:Key="ThemeTextColor">#ffffff</SolidColorBrush>
    <SolidColorBrush x:Key="ThemeBorderColor">#ffffff</SolidColorBrush>

    <ImageSource x:Key="LogoPath">/TrelloApp;component/Resources/app_logo_night.png</ImageSource>
