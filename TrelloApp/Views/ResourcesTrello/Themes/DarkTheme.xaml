    <ImageSource x:Key="EditIconPath">/TrelloApp;component/Resources/edit.png</ImageSource>
    <ImageSource x:Key="EyeIconPath">/TrelloApp;component/Resources/eye.png</ImageSource>
    <ImageSource x:Key="MoveColumnIconPath">/TrelloApp;component/Resources/move-column.png</ImageSource>
    <ImageSource x:Key="MoveColumnLeftIconPath">/TrelloApp;component/Resources/move_left.png</ImageSource>
    <ImageSource x:Key="MoveColumnRightIconPath">/TrelloApp;component/Resources/move_right.png</ImageSource>
    <ImageSource x:Key="AddIconPath">/TrelloApp;component/Resources/plus.png</ImageSource>
